{
  "manifest_version": 2,

  "name": "EnglishTip",
  "description": "English bottom tip.",
  "version": "1.0",

  "browser_action": {
   "default_icon": "englishtip-48.png",
   "default_popup": "popup.html"
  },
  "permissions":["tabs","storage"],
  "content_scripts": [ {
    "matches": [ "http://*/*", "https://*/*"],
    "js": ["content_script.js"]
  } ],
  "icons": { "16": "englishtip-16.png", "48": "englishtip-48.png", "128": "englishtip-128.png" }
}